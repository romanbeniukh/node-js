{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "no-console": 0,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "no-unused-expressions": 0
    }
}
